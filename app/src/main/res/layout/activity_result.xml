                android:id="@+id/textViewReproduction"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView" />

            <TextView
                android:id="@+id/textView43"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Comportement"
                android:textColor="@color/design_default_color_primary_dark"
                android:textSize="18sp"
                android:textStyle="bold|italic" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <TextView
                        android:id="@+id/textView46"
                        android:layout_width="116dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="Type de nage"
                        android:textColor="@color/design_default_color_primary_dark"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView48"
                        android:layout_width="174dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="SociabilitÃ©"
                        android:textColor="@color/design_default_color_primary_dark"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView49"
                        android:layout_width="107dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="Territorial"
                        android:textColor="@color/design_default_color_primary_dark"
                        android:textStyle="bold" />

                </TableRow>

            </TableLayout>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/textViewNage"
                    android:layout_width="116dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="TextView" />

                <TextView
                    android:id="@+id/textViewSociabilite"
                    android:layout_width="174dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="TextView" />

                <TextView
                    android:id="@+id/textViewTerritorial"
                    android:layout_width="107dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="TextView" />
            </TableRow>

        </LinearLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>